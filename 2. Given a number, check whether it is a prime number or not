package newone;

import java.util.Scanner;

public class Praime {

	public static void main(String[] args) {
	
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the your =");
		int t = sc.nextInt();
		int i=2;
			if(t%i==0) {
				System.out.println("No");
			}else {
				System.out.println("Yes");
			}

		
	}
	
	
	
}

