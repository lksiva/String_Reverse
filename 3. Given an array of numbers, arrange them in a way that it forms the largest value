package newone;

import java.util.Scanner;

public class ReverseArray {

	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter YOur Name =");
		int t =sc.nextInt();
		int v[] = new int[t]; 
		String lk ="";
		for(int i=0;i<t;i++){
			 v[i]=sc.nextInt();
		}
		for(int i=v.length-1;i>=0;i--) {	
			lk=lk+v[i];
		}
		System.out.println(lk);

	}
}

